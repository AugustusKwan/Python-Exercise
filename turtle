import turtle as t 

t.pensize(3)
t.pencolor('red')
t.begin_fill()
t.color('red')
t.circle(200)
t.end_fill()
t.goto(0,40)
t.begin_fill()
t.color('white')
t.circle(160)
t.end_fill()
t.goto(0,80)
t.begin_fill()
t.color('red')
t.circle(120)
t.end_fill()
t.goto(0,120)
t.begin_fill()
t.color('blue')
t.circle(80)
t.end_fill()
t.circle(80,36)

t.done()
